{
  "name": "politeia",
  "version": "1.0.0",
  "main": "dist/main.js",
  "module": "src/index.js",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier \"src/**/*.js\"",
    "format": "yarn prettier --write",
    "test:format": "yarn prettier --check",
    "test:eslint": "eslint --ext .js ./src",
    "fix:eslint": "eslint --ext .js ./src --fix",
    "test:ci": "yarn test:format && yarn test:eslint && cross-env jest --no-cache",
    "test:dev": "yarn test:format && yarn test:eslint && cross-env jest --watchAll --no-cache",
    "test": "politeiagui-scripts test",
    "test:coverage": "yarn test:ci; open coverage/lcov-report/index.html",
    "build": "webpack --config webpack.prod.js",
    "bundle-report": "webpack-bundle-analyzer --port 4200 dist/stats.json",
    "start": "politeiagui-scripts start"
  },
  "dependencies": {
    "politeiagui-scripts": "1.0.0",
    "@politeiagui/comments": "1.0.0",
    "@politeiagui/common-ui": "1.0.0",
    "@politeiagui/core": "1.0.0",
    "@politeiagui/statistics": "1.0.0",
    "@politeiagui/ticketvote": "1.0.0",
    "js-file-download": "^0.4.12"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@testing-library/jest-dom": "^5.14.1",
    "jest-extended": "^1.0.0",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}

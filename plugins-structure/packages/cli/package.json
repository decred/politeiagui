{
  "name": "@politeiagui/cli",
  "version": "1.0.0",
  "description": "politeiagui cli",
  "license": "MIT",
  "keywords": [
    "pgui",
    "politeiagui-cli",
    "@politeiagui/cli"
  ],
  "main": "lib/pgui.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prettier": "prettier \"**/*.js\"",
    "format": "yarn prettier --write",
    "test:format": "yarn prettier --check",
    "test:eslint": "eslint --ext .js ./lib",
    "fix:eslint": "eslint --ext .js ./lib",
    "test:ci": "yarn test:format && yarn test:eslint && cross-env NODE_OPTIONS=--experimental-vm-modules jest --no-cache",
    "test:dev": "yarn test:format && yarn test:eslint && cross-env NODE_OPTIONS=--experimental-vm-modules jest --watchAll --no-cache",
    "test": "is-ci \"test:ci\" \"test:dev\"",
    "test:coverage": "yarn test:ci; open coverage/lcov-report/index.html"
  },
  "bin": {
    "pgui": "lib/pgui.js"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4"
  },
  "dependencies": {
    "commander": "^8.3.0"
  }
}

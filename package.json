    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.8",
    "react-select": "^2.4.4",
    "react-transition-group": "^4.1.1",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "showdown": "^1.9.0",
    "snew-classic-ui": "https://github.com/decred/snew-classic-ui",
    "source-map-explorer": "^1.8.0",
    "tweetnacl": "^1.0.1",
    "tweetnacl-util": "^0.15.0",
    "typescript": "^3.5.1",
    "xss-filters": "^1.2.7",
    "yup": "^0.27.0"
  },
  "scripts": {
    "mockapi": "node mockapi.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "cross-env HTTPS=true react-app-rewired start",
    "start-redux-logger": "REACT_APP_USE_REDUX_LOGGER=true cross-env HTTPS=true react-app-rewired start",
    "uidev": "concurrently \"node mockapi.js\" \"react-app-rewired start\"",
    "build": "react-app-rewired build",
    "build-v2": "REACT_APP_V2=true react-app-rewired build",
    "build-staging": "REACT_APP_STAGING=true react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "start-v2": "cross-env HTTPS=true REACT_APP_V2=TRUE react-app-rewired start",
    "lint": "./node_modules/.bin/eslint src",
    "precommit": "lint-staged",
    "test:e2e": "nightwatch -c test/e2e/nightwatch.json"
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "prettier --write",
      "yarn lint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/*.{js,jsx}",
      "src/lib/*.{js,jsx}",
      "src/reducers/*.{js,jsx}",
      "src/selectors/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "chromedriver": "^2.46.0",
    "connect": "^3.7.0",
    "connect-api-mocker": "^1.5.0",
    "cross-env": "^5.0.5",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-react": "^7.13.0",
    "fetch-mock": "^7.3.3",
    "https": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.0",
    "nightwatch": "^1.1.11",
    "pem": "^1.14.2",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "react-test-renderer": "^16.8.6",
    "redux-actions-assertions": "^1.3.0",
    "redux-mock-store": "^1.5.1",
    "selenium-server-standalone-jar": "^3.141.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

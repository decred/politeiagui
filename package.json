{
  "name": "politeiagui",
  "version": "0.1.1",
  "network": "testnet",
  "dependencies": {
    "concurrently": "^4.1.0",
    "core-decorators": "^0.20.0",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.30.1",
    "diff": "^3.5.0",
    "dompurify": "^1.0.8",
    "font-awesome": "^4.7.0",
    "js-file-download": "^0.4.1",
    "js-sha3": "^0.8.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.4",
    "node-less-chokidar": "0.3.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.0",
    "qr-image": "^3.2.0",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-body": "^0.2.0",
    "react-dom": "^16.7.0",
    "react-file-reader": "https://github.com/pgonzalez-santiago/react-file-reader",
    "react-markdown": "^4.0.4",
    "react-mde": "^2.1.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.5",
    "react-select": "^2.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "showdown": "^1.9.0",
    "snew-classic-ui": "https://github.com/decred/snew-classic-ui",
    "source-map-explorer": "^1.6.0",
    "tweetnacl": "^1.0.0",
    "tweetnacl-util": "^0.15.0",
    "xss-filters": "^1.2.7"
  },
  "scripts": {
    "mockapi": "node mockapi.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "cross-env HTTPS=true react-scripts start",
    "start-redux-logger": "REACT_APP_USE_REDUX_LOGGER=true cross-env HTTPS=true react-scripts start",
    "uidev": "concurrently \"node mockapi.js\" \"react-scripts start\"",
    "build": "react-scripts build",
    "build-staging": "REACT_APP_STAGING=true react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src",
    "precommit": "lint-staged",
    "test:e2e": "nightwatch -c test/e2e/nightwatch.json"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "yarn lint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/*.{js,jsx}",
      "src/lib/*.{js,jsx}",
      "src/reducers/*.{js,jsx}",
      "src/selectors/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chromedriver": "^2.45.0",
    "connect": "^3.6.5",
    "connect-api-mocker": "^1.3.3",
    "cross-env": "^5.0.5",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-react": "^7.12.1",
    "fetch-mock": "^6.5.2",
    "https": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "nightwatch": "^1.0.18",
    "pem": "^1.13.2",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0",
    "redux-actions-assertions": "^1.3.0",
    "redux-mock-store": "^1.5.1",
    "selenium-server-standalone-jar": "^3.141.5"
  },
  "proxy": {
    "/api": {
      "target": "https://localhost:4443",
      "secure": false,
      "pathRewrite": {
        "/api": ""
      }
    }
  }
}

    "react-body": "^0.2.0",
    "react-datasheet": "^1.3.14",
    "react-dom": "^16.8.6",
    "react-file-reader": "https://github.com/pgonzalez-santiago/react-file-reader",
    "react-infinite-scroller": "^1.2.4",
    "react-markdown": "^4.0.8",
    "react-mde": "^2.1.2",
    "react-mde-newest": "npm:react-mde",

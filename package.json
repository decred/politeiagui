    "js-sha3": "^0.8.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.4",
    "normalize.css": "^8.0.1",
    "pi-ui": "https://github.com/tiagoalvesdulce/pi-ui",
    "prop-types": "^15.7.2",
    "qr-image": "^3.2.0",
